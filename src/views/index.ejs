    <link rel="stylesheet" href="css/bootstrap.min.css">	
    <link rel="stylesheet" href="css/style.css">
</head>
<body>
    
    <!-- top navbar -->
        
    <nav class="sticky-top">
        
          
        <nav class="navbar navbar-expand-md navbar-custom" style="background-color: rgb(198, 248, 244);">
          
         <a href="/" class="navbar-brand"></a>
         <div class="collapse navbar-collapse" id="collapsibleNavbar">
             <ul class="navbar-nav">
             
                 <%for(i=0;i<nav.length;i++){%>
                 <li class="nav-item">
                    <%if (nav[i].name=='Home'){%>
                        <a class="nav-link" href="<%=nav[i].link%>"><%=nav[i].name%></a>
                    <%}
                    else{%>
                        <a class="nav-link" href="<%=nav[i].link%>"><%=nav[i].name%></a>
                   <% } %>                    
                 </li>
                <%}%>
             </ul>
         </div>
     </nav>
 </nav>
 <!--navbar ends-->

<div class="jumbotron jumbotron-custom">
    <div>
        <h1 style="color: white;">My Library</h1></div>
    </div>
    <div class="content">
        <div class="base">
            <div class="top">
                <!-- card 1 start  -->
                <div class="card card-custom mb-3" style="max-width: 900px;">
                    <div class="row no-gutters">
                        <div class="col-md-4">
                            <img src="./images/lib1.png"  style="width:150px;height:150px;margin-bottom: 9px;" class="img-circle" class="card-img" alt="...">
                        </div>
                        <div class="col-md-8">
                            <div class="card-body">
                                
                                <p class="card-text"><strong>"If you have a garden and a library, you have everything you need"
                                           - Marcus Tullius Cicero</strong></p>
                                <p class="card-text"> <small style="font-family:courier,'new courier';" class="text-muted"></small></p>
                            </div>
                        </div>
                    </div>
              </div>
              <!-- card 1 ends  -->
              <!-- card 2 start -->          
              <div class="card card-custom mb-3" style="max-width: 900px;">
                <div class="row no-gutters">
                    <div class="col-md-4">
                        <img src="./images/lib2.jpg" style="width:150px;height:150px;margin-bottom: 9px;" class="img-circle" class="card-img" alt="...">
                    </div>
                    <div class="col-md-8">
                        <div class="card-body">
                            <p class="card-text"><strong>"The library is the temple of learning, and learning has liberated more people than all the wars in history"
                               - Carl T. Rowan</strong></p>
                            <p class="card-text"><small style="font-family:courier,'new courier';" class="text-muted"></small></p>
                        </div>
                    </div>
                </div>
            </div>
            <!-- card 2 ends-->
            
        
        <!-- footer starts here -->
        <footer>
            <div class="content container-fluid">
                <div class="row">
                    <div class="col-sm-5">
                        <p> &copy;All Rights Reserved</p>
                    </div> 
                    <br>
                    
                    <div class="col-md-3">
                        <nav class="navbar navbar-default" role="navigation">
                            <a href="#" target="_blank"><span class="fa-stack fa-lg">
                                <i class="fa fa-circle fa-stack-2x "></i>
                                <i class="fa fa-facebook fa-stack-1x fa-inverse "></i>
                            </span></a>
                            <a href="#" target="_blank"><span class="fa-stack fa-lg">
                                <i class="fa fa-circle fa-stack-2x "></i>
                                <i class="fa fa-twitter fa-stack-1x fa-inverse "></i>
                            </span></a>
                            <a href="#" target="_blank"><span class="fa-stack fa-lg">
